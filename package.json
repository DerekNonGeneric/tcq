{
  "name": "tcq",
  "version": "1.0.0",
  "description": "A TC39 Discussion Queue",
  "main": "dist/server/server.js",
  "scripts": {
    "build-frontend": "webpack --config ./src/client/webpack.config.js",
    "build-frontend-prod": "webpack -p --config ./src/client/webpack.config.js",
    "build-frontend-watch": "webpack --watch --config ./src/client/webpack.config.js",
    "build-backend": "tsc -p ./src/server",
    "iterate":
      "npm run build-backend && concurrently \"npm run build-frontend-watch\" \"node ./dist/server/server.js\"",
    "postinstall": "npm run build-backend && npm run build-frontend-prod"
  },
  "author": "Brian Terlson",
  "contributors": [
    {
      "name": "Daniel Rosenwasser",
      "url": "https://github.com/DanielRosenwasser"
    }
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": "8.0"
  },
  "dependencies": {
    "body-parser": "^1.18.1",
    "connect-ensure-login": "^0.1.1",
    "documentdb": "^1.12.2",
    "documentdb-session": "^1.0.1",
    "documentdb-typescript": "^1.0.6",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "github": "^11.0.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "uuid": "^3.1.0",
    "vue": "github:herringtondarkholme/vue#new-type",
    "@types/connect-ensure-login": "^0.1.3",
    "@types/documentdb": "^1.10.2",
    "@types/documentdb-session": "^1.0.0",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/node": "^8.0.28",
    "@types/passport": "^0.3.4",
    "@types/passport-github": "^1.1.1",
    "@types/socket.io": "^1.4.30",
    "@types/socket.io-client": "^1.4.30",
    "@types/uuid": "^3.4.2",
    "concurrently": "^3.5.0",
    "html-webpack-plugin": "^2.30.1",
    "ts-loader": "^2.3.7",
    "typescript": "^2.6.0-dev.20170920",
    "webpack": "^3.6.0"
  }
}
